	public void goTo_inGameMenu() throws IOException///////////////////////////////////////////////////////// This method goes to the in game menu
	{//////////////////////////////////////////////////////////////////////////////////////////////////////// 
		GeneralMethods gm = new GeneralMethods();//////////////////////////////////////////////////////////// Access to the general methods
		MainMenu mainMenu = new MainMenu();////////////////////////////////////////////////////////////////// Access to the Main Menu
		do/////////////////////////////////////////////////////////////////////////////////////////////////// 
		{//////////////////////////////////////////////////////////////////////////////////////////////////// Start of do while loop
			done = false;//////////////////////////////////////////////////////////////////////////////////// 
			changeInGameOptions_Main();////////////////////////////////////////////////////////////////////// 
			menuAnswer = printInGameMenu();////////////////////////////////////////////////////////////////// 
			switch (menuAnswer)////////////////////////////////////////////////////////////////////////////// Start of switch/case
			{//////////////////////////////////////////////////////////////////////////////////////////////// 
			case "1"://////////////////////////////////////////////////////////////////////////////////////// case "1": Explore
			case "E":////////////////////////////////////////////////////////////////////////////////////////
				String answer = getMenuAreaName();///////////////////////////////////////////////////////////
				if (answer == "    THE COLONY    ")
				{
					TheColony theColony = new TheColony();
					theColony.goTo_TheColony();////////////////////////////////////////////////////////////// Goes to the explore menu
				}
				else if (answer == " SEREDIE'S PLAINE ")
				{
					Seredie seredie = new Seredie();
					seredie.goTo_Seredie();
				}
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "1": Explore

			case "2"://////////////////////////////////////////////////////////////////////////////////////// case "2": Travel
			case "T":////////////////////////////////////////////////////////////////////////////////////////
				goTo_Travel();
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "2": Travel

			case "3"://////////////////////////////////////////////////////////////////////////////////////// case "3": Check goals
			case "G":////////////////////////////////////////////////////////////////////////////////////////
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "3": Goals

			case "4"://////////////////////////////////////////////////////////////////////////////////////// case "4": Check inventory
			case "I":////////////////////////////////////////////////////////////////////////////////////////
				goTo_Inventory();////////////////////////////////////////////////////////////////////////////
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "4": Inventory

			case "5"://////////////////////////////////////////////////////////////////////////////////////// case "4": Check inventory
			case "Q":////////////////////////////////////////////////////////////////////////////////////////
				System.out.print("Are you sure you want to go back to the main menu? ");/////////////////////
				menuAnswer = gm.promptUser();////////////////////////////////////////////////////////////////
				if (menuAnswer.equals("Y"))//////////////////////////////////////////////////////////////////
					mainMenu.mainMenu_Continue();//////////////////////////////////////////////////////////// 
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "Q": Quit
			

			case "6"://////////////////////////////////////////////////////////////////////////////////////// case "Q": Goes to main menu
			case "B":////////////////////////////////////////////////////////////////////////////////////////
				answer = getMenuAreaName();///////////////////////////////////////////////////////////
				if (answer == "    THE COLONY    ")
				{
					TheColony theColony = new TheColony();
					theColony.goTo_TheColony();////////////////////////////////////////////////////////////// Goes to the explore menu
				}
				else if (answer == " SEREDIE'S PLAINE ")
				{
					Seredie seredie = new Seredie();
					seredie.goTo_Seredie();
				}
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "4": Inventory
			

			default:///////////////////////////////////////////////////////////////////////////////////////// If invalid answer just reloops the menu
				break;/////////////////////////////////////////////////////////////////////////////////////// End of Default case


			}//////////////////////////////////////////////////////////////////////////////////////////////// End of switch/case
		}//////////////////////////////////////////////////////////////////////////////////////////////////// End of do while loop
		while (done != true);////////////////////////////////////////////////////////////////////////////////
	}//////////////////////////////////////////////////////////////////////////////////////////////////////// goTo_inGameMenu() End.




	public void goTo_Travel() throws IOException//////////////////////////////////////////////////////////// This method goes to the in game menu
	{//////////////////////////////////////////////////////////////////////////////////////////////////////// 
		GeneralMethods gm = new GeneralMethods();//////////////////////////////////////////////////////////// Access to the general methods
		MainMenu mainMenu = new MainMenu();////////////////////////////////////////////////////////////////// Access to the Main Menu
		do/////////////////////////////////////////////////////////////////////////////////////////////////// 
		{//////////////////////////////////////////////////////////////////////////////////////////////////// Start of do while loop
			done = false;//////////////////////////////////////////////////////////////////////////////////// 
			changeInGameOptions_Travel();/////////////////////////////////////////////////////////////////// 
			menuAnswer = printInGameMenu();////////////////////////////////////////////////////////////////// 
			switch (menuAnswer)////////////////////////////////////////////////////////////////////////////// Start of switch/case
			{//////////////////////////////////////////////////////////////////////////////////////////////// 
			case "1"://////////////////////////////////////////////////////////////////////////////////////// case "1": Travel to the colony
				System.out.print("Are you sure you want to travel? ");///////////////////////////////////////
				menuAnswer = gm.promptUser();////////////////////////////////////////////////////////////////
				if (menuAnswer.equals("Y"))//////////////////////////////////////////////////////////////////
				{////////////////////////////////////////////////////////////////////////////////////////////
					TheColony theColony = new TheColony();/////////////////////////////////////////////////// Access to the Colony
					theColony.goTo_TheColony();////////////////////////////////////////////////////////////// Goes to the explore menu
				}////////////////////////////////////////////////////////////////////////////////////////////
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "1": the colony

			case "2"://////////////////////////////////////////////////////////////////////////////////////// case "2": Travel to the plaines of Seredie
					Seredie seredie = new Seredie();
					seredie.goTo_Seredie();
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "2": plaines of Seredie

			case "3"://////////////////////////////////////////////////////////////////////////////////////// case "3": Travel to mt. shober
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "3": mt. shober

			case "4"://////////////////////////////////////////////////////////////////////////////////////// case "4": Travel to the tooshenair tundra
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "4": tooshenair tundra

			case "5"://////////////////////////////////////////////////////////////////////////////////////// case "5": Travel to the asternene desert
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "5": asternene desert

			case "6"://////////////////////////////////////////////////////////////////////////////////////// case "6": Travel to the orlone wetlands
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "6": orlone wetlands

			case "7"://////////////////////////////////////////////////////////////////////////////////////// case "7": Travel to the morlasta exile colony
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "7": morlasta exile colony

			case "8"://////////////////////////////////////////////////////////////////////////////////////// case "8": Goes in game menu
			case "B":
				goTo_inGameMenu();/////////////////////////////////////////////////////////////////////////// 
				break;/////////////////////////////////////////////////////////////////////////////////////// End of case "8": Back

			default:///////////////////////////////////////////////////////////////////////////////////////// If invalid answer just reloops the menu
				break;/////////////////////////////////////////////////////////////////////////////////////// End of Default case


			}//////////////////////////////////////////////////////////////////////////////////////////////// End of switch/case
		}//////////////////////////////////////////////////////////////////////////////////////////////////// End of do while loop
		while (done != true);////////////////////////////////////////////////////////////////////////////////
	}//////////////////////////////////////////////////////////////////////////////////////////////////////// goTo_Exolore() End.




	public void goTo_Inventory() throws IOException///////////////////////////////////////////////////////// This method goes to the in game menu
